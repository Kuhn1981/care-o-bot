<?xml version="1.0"?>
<launch>

	<rosparam file="$(find cob3_defs)/ros/controller/arm_controller.yaml" command="load"/>

	<node name="spawn_arm_controller" pkg="pr2_controller_manager" type="spawner" args="--wait-for=/calibrated
		arm_controller
		" respawn="false" output="screen" />
	
	<group ns="arm_controller">
		<node name="joint_trajectory_action_node" pkg="joint_trajectory_action" type="joint_trajectory_action" />
	</group>

</launch>
