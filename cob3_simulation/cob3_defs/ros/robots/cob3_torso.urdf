			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0.0005"/>
			<geometry>
		        <box size="0.001 0.001 0.001"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0.0005"/>
			<geometry>
		        <box size="0.001 0.001 0.001"/>
			</geometry>
		</collision>
	</link><link name="link_head">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<geometry>
		        <box size="0.1 0.1 0.1"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/head.stl" /> -->
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<geometry>
		        <box size="0.1 0.1 0.1"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/head.stl"/> -->
			</geometry>
		</collision>
	</link><link name="link_eyes">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0.005"/>
			<geometry>
		        <box size="0.1 0.1 0.01"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/finger_tip.stl" /> -->
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0.005"/>
			<geometry>
		        <box size="0.1 0.1 0.01"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/finger_tip.stl"/> -->
			</geometry>
		</collision>
	</link><link name="link_colorcameraLeft">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<geometry>
		        <box size="0.1 0.1 0.1"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/finger_tip.stl" /> -->
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<geometry>
		        <box size="0.1 0.1 0.1"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/finger_tip.stl"/> -->
