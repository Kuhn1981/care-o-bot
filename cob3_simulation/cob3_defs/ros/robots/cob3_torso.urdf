<?xml version="1.0" ?><robot name="cob3_torso" xmlns:controller="http://playerstage.sourceforge.net/gazebo/xmlschema/#controller" xmlns:interface="http://playerstage.sourceforge.net/gazebo/xmlschema/#interface" xmlns:sensor="http://playerstage.sourceforge.net/gazebo/xmlschema/#sensor">

	<!-- include gazebo defs-->
	<gazebo>
		<controller:gazebo_ros_time name="gazebo_ros_time" plugin="libgazebo_ros_time.so">
			<alwaysOn>true</alwaysOn>
			<updateRate>100.0</updateRate>
			<interface:audio name="dummy_gazebo_ros_time_iface_should_not_be_here"/>
		</controller:gazebo_ros_time>
      
		<controller:gazebo_ros_controller_manager name="gazebo_ros_controller_manager" plugin="libgazebo_ros_controller_manager.so">
			<alwaysOn>true</alwaysOn>
			<updateRate>1000.0</updateRate>
			<interface:audio name="gazebo_ros_controller_manager_dummy_iface"/>
		</controller:gazebo_ros_controller_manager>
	</gazebo>
	
	<!-- create joint to world -->
	<joint name="joint_world_torso" type="fixed">
		<origin rpy="0 0 0" xyz="0 0 0.1126"/>
		<parent link="world"/>
		<child link="link_torso"/>
	</joint>
	
	<!-- include torso defs-->
	<link name="link_torso">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<geometry>
		        <box size="0.1 0.1 0.1"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/torso.stl" /> -->
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<geometry>
		        <box size="0.1 0.1 0.1"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/torso.stl"/> -->
			</geometry>
		</collision>
	</link><link name="link_tray">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<geometry>
		        <box size="0.1 0.1 0.1"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/tray.stl" /> -->
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<geometry>
		        <box size="0.1 0.1 0.1"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/tray.stl"/> -->
			</geometry>
		</collision>
	</link><link name="link_neckZ">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="0.001"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0.0005"/>
			<geometry>
		        <box size="0.001 0.001 0.001"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0.0005"/>
			<geometry>
		        <box size="0.001 0.001 0.001"/>
			</geometry>
		</collision>
	</link><link name="link_neck">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
		</inertial>
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<geometry>
		        <box size="0.1 0.1 0.1"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/neck.stl" /> -->
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<geometry>
		        <box size="0.1 0.1 0.1"/>
<!--				<mesh filename="package://cob3_defs/common/files/meshes/neck.stl"/> -->
			</geometry>
		</collision>
	</link><link name="link_headZ">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
