<link name="Palm">
	<inertial>
		<origin xyz="0 0 0" rpy="0 0 0"/> <!-- needs to be C.O.G -->
		<mass value="1"/>							<!--review -->
		<inertia ixx="100"  ixy="0"  ixz="0" iyy="100" iyz="0" izz="100" />
	</inertial>

	<visual>
		<origin xyz="0 0 0" rpy="0 0 0" />
		<!--<origin xyz="0 0 0.0386" rpy="0 0 0" />-->
		<geometry>
			<!--<cylinder radius="0.065" length="0.0772" />--> <mesh filename="$(find cob3_defs)/common/files/meshes/Palm.stl" />
		</geometry>
	</visual>

	<collision>
		<origin xyz="0 0 0.0386" rpy="0 0 0" />
		<geometry>
			<cylinder radius="0.065" length="0.0772" /> <!--cylinder approx. -->
		</geometry>
		<contact_coefficient mu="0" k_p="1000.0" k_d="1.0"/> <!--review -->
	</collision>
</link>

