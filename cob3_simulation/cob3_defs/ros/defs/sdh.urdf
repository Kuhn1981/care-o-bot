<joint name="World_palm" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="world"/>
    <child link="Palm"/>
 </joint>
 
 
 <joint name="palm_finger1-1" type="revolute">  <!-- make the second finger mimic this one -->
    <!--<origin xyz="-0.033 -0.019053 0.017" rpy="1.570796327 0 -1.570796327" /> -->
     <origin xyz="0.01905 0.033 0.0772" rpy="0 0 3.159265" />
        <parent link="Palm"/>
    <child link="Finger1-1"/>
	<axis xyz="0 0 1" /> <!--0 1 0 -->
    <calibration reference_position="0.0"/>
    <dynamics damping="0.0" friction="0.0"/>
    <limit effort="30" velocity="1.0" upper="1.57079633" lower="0" /> <!-- rads. effort is N-m applied force. vel is rad/s. -->
    <safety_controller k_velocity="10"/>
 </joint>
 
